{
  "name": "Github Branch Commands",
  "version": "1.0",
  "description": "Lets you copy the command to checkout a branch from pull requests on github",
  "permissions": ["tabs", "declarativeContent", "storage"],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.3.1.min.js", "content.js"],
    "matches": ["https://github.com/*/*/pull/*"]
  }],
  "manifest_version": 2
}
